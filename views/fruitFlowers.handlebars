    <body>

    <div class=Fruittitle>
      <h2>FLOWERS:</h2>
      <div class="box">
        <ul>
        <table>
    <thead> 
        <th>Fruit</th> 
        <th>Price</th> 
    </thead> 
    <tbody>
        {{#each fruit}} 
        <tr> 
            <td>{{fruit_name}}</td>
            <td>{{price}}</td>  
        </tr> 
        {{/each}} 
    </tbody> 
    </table> 
      </div>


    <div class=Flowertitle>
      <h2>FLOWERS:</h2>
      <div class="box">
        <ul>
        <table>
    <thead> 
        <th>Flowers</th>  
        <th>Color</th> 
    </thead> 
    <tbody>
        {{#each flower}} 
        <tr> 
            <td>{{flower_name}}</td>
            <td>{{color}}</td>  
        </tr> 
        {{/each}} 
    </tbody> 
    </table> 
      </div>
    </div>

  </body>
